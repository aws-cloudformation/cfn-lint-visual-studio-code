{
  "typeName": "AWS::IAM::RolePolicy",
  "description": "Schema for IAM Role Policy",
  "additionalProperties": false,
  "sourceUrl": "https://github.com/aws-cloudformation/aws-cloudformation-resource-providers-iam.git",
  "properties": {
    "PolicyDocument": {
      "description": "The policy document.",
      "type": "object",
      "markdownDescription": "The policy document.\n\n---\n\nRequired: No  \nUpdate requires: No interruption"
    },
    "PolicyName": {
      "description": "The friendly name (not ARN) identifying the policy.",
      "type": "string",
      "markdownDescription": "The friendly name (not ARN) identifying the policy.\n\n---\n\nRequired: Yes  \nType: String  \nUpdate requires: Replacement"
    },
    "RoleName": {
      "description": "The name of the policy document.",
      "type": "string",
      "markdownDescription": "The name of the policy document.\n\n---\n\nRequired: Yes  \nType: String  \nUpdate requires: Replacement"
    }
  },
  "required": [
    "PolicyName",
    "RoleName"
  ],
  "primaryIdentifier": [
    "/properties/PolicyName",
    "/properties/RoleName"
  ],
  "createOnlyProperties": [
    "/properties/PolicyName",
    "/properties/RoleName"
  ],
  "tagging": {
    "taggable": false,
    "tagOnCreate": false,
    "tagUpdatable": false,
    "cloudFormationSystemTags": false
  },
  "handlers": {
    "create": {
      "permissions": [
        "iam:PutRolePolicy",
        "iam:GetRolePolicy"
      ]
    },
    "read": {
      "permissions": [
        "iam:GetRolePolicy"
      ]
    },
    "update": {
      "permissions": [
        "iam:PutRolePolicy",
        "iam:GetRolePolicy"
      ]
    },
    "delete": {
      "permissions": [
        "iam:DeleteRolePolicy",
        "iam:GetRolePolicy"
      ]
    }
  },
  "attributes": {}
}