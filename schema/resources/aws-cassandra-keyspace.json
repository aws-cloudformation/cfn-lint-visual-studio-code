{
  "typeName": "AWS::Cassandra::Keyspace",
  "description": "Resource schema for AWS::Cassandra::Keyspace",
  "properties": {
    "KeyspaceName": {
      "description": "Name for Cassandra keyspace",
      "type": "string",
      "pattern": "^[a-zA-Z0-9][a-zA-Z0-9_]{1,47}$",
      "markdownDescription": "Name for Cassandra keyspace\n\n---\n\nRequired: No\n\nType: string    \n\nPattern: ^[a-zA-Z0-9][a-zA-Z0-9_]{1,47}$  \n\nUpdate requires: No interruption "
    },
    "Tags": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "$ref": "#/definitions/Tag"
      },
      "minItems": 0,
      "maxItems": 50,
      "markdownDescription": "\n\n---\n\nRequired: No\n\nType: array     \n\nUpdate requires: No interruption "
    }
  },
  "definitions": {
    "Tag": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "Key": {
          "type": "string",
          "minLength": 1,
          "maxLength": 128
        },
        "Value": {
          "type": "string",
          "minLength": 1,
          "maxLength": 256
        }
      },
      "required": [
        "Value",
        "Key"
      ]
    }
  },
  "additionalProperties": false,
  "required": [],
  "createOnlyProperties": [
    "/properties/KeyspaceName"
  ],
  "primaryIdentifier": [
    "/properties/KeyspaceName"
  ],
  "handlers": {
    "create": {
      "permissions": [
        "cassandra:Create",
        "cassandra:Select",
        "cassandra:TagResource"
      ]
    },
    "read": {
      "permissions": [
        "cassandra:Select"
      ]
    },
    "update": {
      "permissions": [
        "cassandra:Alter",
        "cassandra:Select",
        "cassandra:TagResource",
        "cassandra:UntagResource"
      ]
    },
    "delete": {
      "permissions": [
        "cassandra:Drop",
        "cassandra:Select"
      ]
    },
    "list": {
      "permissions": [
        "cassandra:Select"
      ]
    }
  }
}